{
    "_inherit": true,
    "_variables": {
        "@TitleSize": "16px",
        "@TitleColor": "#363636",
        "@SubtitleTextSize": "14px",
        "@SubtitleColor": "#000000",
        "@TextSize": "12px",
        "@TextColor": "#000000",
        "@FooterTextSize": "10px",
        "@FooterColor": "#000000",
        "@Background.color": "white",
        "@GridColor": "#FFFFFF",
        "@Object.title.color": "darkgray",
        "@Object.title.size": "12px",
        "@Object.axis.line.color": "#EEEE",
        "@Colors.primary": "#8daacb",
        "@Colors.others": "#50A0F0",
        "@Colors.error": "#bb4a4a",
        "@Colors.null": "#CCCCCC"
    },
    "color": "@TextColor",
    "fontSize": "@TextSize",
    "gridColor": "@GridColor",
    "backgroundColor": "@Background.color",
    "customStyles": [{
        "cssRef": "theme.css",
        "classRef": "sense-theme"
    }],
    "object": {
        "title": {
            "main": {
                "color": "@TitleColor",
                "fontSize": "@TitleSize"
            },
            "subTitle": {
                "color": "@SubtitleColor",
                "fontSize": "@SubtitleTextSize"
            },
            "footer": {
                "color": "@FooterColor",
                "fontSize": "@FooterTextSize"
            }
        },
        "label": {
            "name": {
                "color": "@TextColor",
                "fontSize": "@TextSize"
            },
            "value": {
                "color": "@TextColor",
                "fontSize": "@TextSize"
            }
        },
        "axis": {
            "title": {
                "color": "@Object.title.color",
                "fontSize": "@Object.title.size"
            },
            "label": {
                "color": "@TextColor",
                "fontSize": "@TextSize"
            },
            "line": {
                "major": {
                    "color": "@Object.axis.line.color"
                },
                "minor": {
                    "color": "@Object.axis.line.color"
                }
            }
        },
        "grid": {
            "line": {
                "highContrast": {
                    "color": "@Object.axis.line.color"
                },
                "major": {
                    "color": "@Object.axis.line.color"
                },
                "minor": {
                    "color": "@Object.axis.line.color"
                }
            }
        },
        "referenceLine": {
            "label": {
                "name": {
                    "color": "@Object.title.color",
                    "fontSize": "@TextSize"
                }
            },
            "outOfBounds": {
                "color": "@Object.title.color",
                "backgroundColor": "@GridColor",
                "fontSize": "@TextSize"
            }
        },
        "legend": {
            "title": {
                "color": "@Object.title.color",
                "fontSize": "@TextSize"
            },
            "label": {
                "color": "@Object.title.color",
                "fontSize": "@TextSize"
            }
        }
    },
    "dataColors": {
        "primaryColor": "@Colors.primary",
        "othersColor": "@Colors.others",
        "errorColor": "@Colors.error",
        "nullColor": "@Colors.null"
    },
